{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "vh-datafactory-dev"
		},
		"vhreporting_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'vhreporting_blob'"
		},
		"vhreporting-kv-link_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://vhcoreinfradev.vault.azure.net/"
		},
		"vhreporting-sds-kv-link_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://vh-infra-core-dev.vault.azure.net/"
		},
		"vhreporting_link_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "VhReportingDatabaseConnectionString"
		},
		"vhvideo_link_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "connectionstrings--vhvideo"
		},
		"AzureBlobStorage958_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/705b2731-0e0b-4df7-8630-95f157f0a347/resourceGroups/vh-core-infra-dev/providers/Microsoft.Storage/storageAccounts/vhcoreinfradev"
		},
		"AzureBlobStorage958_properties_groupId": {
			"type": "string",
			"defaultValue": "blob"
		},
		"AzureBlobStorage958_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"vhcoreinfradev.blob.core.windows.net"
			]
		},
		"vhadfendpoint_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/705b2731-0e0b-4df7-8630-95f157f0a347/resourceGroups/vh-core-infra-dev/providers/Microsoft.Sql/servers/vh-core-infra-dev"
		},
		"vhadfendpoint_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"vhadfendpoint_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"vh-core-infra-dev.database.windows.net"
			]
		},
		"vhadfsdsendpoint_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/867a878b-cb68-4de5-9741-361ac9e178b6/resourceGroups/vh-infra-core-dev/providers/Microsoft.Sql/servers/vh-infra-core-dev"
		},
		"vhadfsdsendpoint_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"vhadfsdsendpoint_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"vh-infra-core-dev.database.windows.net"
			]
		},
		"vhcoreinfrasbox_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/705b2731-0e0b-4df7-8630-95f157f0a347/resourceGroups/vh-core-infra-sandbox/providers/Microsoft.Sql/servers/vh-core-infra-sandbox"
		},
		"vhcoreinfrasbox_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "vh-datafactory-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"vhreporting_blob_connectionString": {
						"value": "[parameters('vhreporting_blob_connectionString')]"
					},
					"vhreporting-kv-link_properties_typeProperties_baseUrl": {
						"value": "[parameters('vhreporting-kv-link_properties_typeProperties_baseUrl')]"
					},
					"vhreporting-sds-kv-link_properties_typeProperties_baseUrl": {
						"value": "[parameters('vhreporting-sds-kv-link_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStorage958_properties_privateLinkResourceId": {
						"value": "[parameters('AzureBlobStorage958_properties_privateLinkResourceId')]"
					},
					"AzureBlobStorage958_properties_groupId": {
						"value": "[parameters('AzureBlobStorage958_properties_groupId')]"
					},
					"AzureBlobStorage958_properties_fqdns": {
						"value": "[parameters('AzureBlobStorage958_properties_fqdns')]"
					},
					"vhadfendpoint_properties_privateLinkResourceId": {
						"value": "[parameters('vhadfendpoint_properties_privateLinkResourceId')]"
					},
					"vhadfendpoint_properties_groupId": {
						"value": "[parameters('vhadfendpoint_properties_groupId')]"
					},
					"vhadfendpoint_properties_fqdns": {
						"value": "[parameters('vhadfendpoint_properties_fqdns')]"
					},
					"vhadfsdsendpoint_properties_privateLinkResourceId": {
						"value": "[parameters('vhadfsdsendpoint_properties_privateLinkResourceId')]"
					},
					"vhadfsdsendpoint_properties_groupId": {
						"value": "[parameters('vhadfsdsendpoint_properties_groupId')]"
					},
					"vhadfsdsendpoint_properties_fqdns": {
						"value": "[parameters('vhadfsdsendpoint_properties_fqdns')]"
					},
					"vhcoreinfrasbox_properties_privateLinkResourceId": {
						"value": "[parameters('vhcoreinfrasbox_properties_privateLinkResourceId')]"
					},
					"vhcoreinfrasbox_properties_groupId": {
						"value": "[parameters('vhcoreinfrasbox_properties_groupId')]"
					},
					"vhreporting_link_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('vhreporting_link_properties_typeProperties_connectionString_secretName')]"
					},
					"vhvideo_link_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('vhvideo_link_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "vh-datafactory-dev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'vh-datafactory-dev_ArmTemplate_0')]"
			]
		}
	]
}